<!doctype html>
<html>
  <head>
    <title>Welcome to <%=shopName%></title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
  </head>
  <body>
    <h1><%=shopName%> Shop</h1>
    <h2>Here are the users we have:</h2>
    <ul>
      <% availableUsers.forEach(function(UserDetails) { %>
        <li>Username: <%= UserDetails.username %>, 
            First name: <%=UserDetails.firstname%>,
            Last name: <%=UserDetails.lastname%>
            Email: <%=UserDetails.email%></li>
      <% }) %>
    </ul>
    </body>
</html>
